{
	"domains": [
		{"name": "appinmove.com"},
		{"name": "appinmove.fr"},
		{"name": "bs-partners.fr"}
	],
	"framework": { 
		"root": "/mnt/c/webroot/",
		"version": "1.1.1"
	},
	"template": {
		"engine": "Smarty",
		"version": "3.1.30",
		"extension": ".tpl"
	},
	"languages":[
		{
			"iso": "fr-FR",
			"default": true
		}
	],
	"assets": [
		{
			"type": "npm",
			"name": "jquery",
			"version": "3.1.0",
			"resources": [
				{
					"type": "javascript",
					"file": "dist/jquery.min.js"
				}
			]
		},
		{
			"type": "npm",
			"name": "bootstrap",
			"version": "3.3.7",
			"resources": [
				{
					"type": "css",
					"file": "dist/css/bootstrap.min.css"
				},
				{
					"type": "javascript",
					"file": "dist/js/bootstrap.min.js"
				}
			]
		},
		{
			"type": "styles",
			"name": "icomoon",
			"version": "1.0",
			"resources": [
				{
					"type": "css",
					"file": "style.css"
				}
			]
		},
		{
			"type": "npm",
			"name": "animate.css",
			"version": "3.5.2",
			"resources": [
				{
					"type": "css",
					"file": "animate.min.css"
				}
			]
		},
		{
			"type": "styles",
			"name": "",
			"version": "",
			"resources": [
				{
					"type": "css",
					"file": "bsp.css"
				}
			]
		},
		{
			"type": "npm",
			"name": "jquery-ui-dist",
			"version": "1.12.0",
			"resources": [
				{
					"type": "javascript",
					"file": "jquery-ui.min.js"
				},
				{
					"type": "css",
					"file": "jquery-ui.min.css"
				},
				{
					"type": "css",
					"file": "jquery-ui.structure.min.css"
				},
				{
					"type": "css",
					"file": "jquery-ui.theme.min.css"
				}			
			]
		},
		{
			"type": "framework",
			"name": "maskedinput",
			"version": "1.0",
			"resources": [
				{
					"type": "javascript",
					"file": "jquery.maskedinput.min.js"
				}
			]
		},
		{
			"type": "framework",
			"name": "app",
			"version": "1.0",
			"resources": [
				{
					"type": "javascript",
					"file": "app.js"
				}
			]
		},
		{
			"type": "npm",
			"name": "moment",
			"version": "2.15.1",
			"resources": [
				{
					"type": "javascript",
					"file": "min/moment.min.js"
				}
			]
		}
	]
}